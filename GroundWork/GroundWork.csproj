<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Alin Andersen</Authors>
    <Description>Collection of useful utility and helper classes.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>Github</RepositoryType>
    <RepositoryUrl>https://github.com/onixion/GroundWork</RepositoryUrl>
    <PackageProjectUrl>https://github.com/onixion/GroundWork</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/onixion/GroundWork/master/Assets/logo.png</PackageIconUrl>
    <PackageLicenseFile></PackageLicenseFile>
    <Version>1.0.3</Version>
  </PropertyGroup>

</Project>
