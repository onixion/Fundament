<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alin Andersen</Authors>
    <Description>Collection of useful utility and helper classes for exceptions.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <PackageProjectUrl>https://github.com/onixion/GroundWork</PackageProjectUrl>
    <RepositoryUrl>https://github.com/onixion/GroundWork</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>common utility helper collection exceptions</PackageTags>
    <Version>1.0.5</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Assets\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Groundwork\GroundWork.csproj" />
  </ItemGroup>

</Project>
